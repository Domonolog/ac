.loader-details__block {
  padding-top: 70px;
  padding-bottom: 70px;
  display: flex;
  justify-content: center;
  position: absolute;
  width: 328px;
}

.loader-details {
  border: 4px solid rgba(0, 0, 0, 0.1);
  width: 70px;
  height: 70px;
  border-radius: 50%;
  border-bottom: 4px solid #1dbe7a;
  animation: loader-details 2s linear infinite;
  box-shadow: 0 -1px 4px rgba(0, 0, 0, 0.1);
  display: flex;
  justify-content: center;
}

.mobile-details.active {
  .loader-details__block {
    display: flex;
  }
}

@keyframes loader-details {
  0% {
    transform: rotate(0deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.left-sidebar__fixed {
  max-width: 330px;
  z-index: 51;
  height: 100vh;
  position: fixed;
  overflow-x: scroll;

  &::-webkit-scrollbar {
    width: 0;
  }

  &::-webkit-scrollbar-thumb {
    width: 0;
  }

  &::-webkit-scrollbar-track {
    width: 0;
  }
}

.section-details {
